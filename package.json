{
  "name": "@kerim-keskin/react-masterpass",
  "version": "1.0.5",
  "description": "masterpass integration for react apps",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run prettier && npm run lint",
    "rollup": "rollup -c",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerimkeskin/react-masterpass.git"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "typescript",
    "masterpass",
    "react-masterpass"
  ],
  "author": "Kerim Keskin (@kerimkeskin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kerimkeskin/react-masterpass/issues"
  },
  "homepage": "https://github.com/kerimkeskin/react-masterpass#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "axios": "^1.1.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "rollup": "^3.2.5",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "axios": ">=0.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
